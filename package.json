{
  "name": "ilazy",
  "version": "0.0.1",
  "description": "一个通用的适合懒惰的人的交互包",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "test": "vitest",
    "dev": "esno src/index.ts",
    "test:watch": "vitest --watch",
    "coverage": "vitest --coverage",
    "prepublishOnly": "npm run build",
    "build:watch": "npm run build -- --watch src",
    "build": "tsup src/index.ts --format cjs,esm --dts --clean"
  },
  "keywords": [
    "node",
    "utils",
    "typescript"
  ],
  "author": "markthree",
  "license": "MIT",
  "devDependencies": {
    "@markthree/node-shared": "^1.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.1.3",
    "@types/node": "^17.0.8",
    "chalk": "^5.0.0",
    "commander": "^8.3.0",
    "esno": "^0.14.0",
    "execa": "^6.0.0",
    "fs-extra": "^10.0.0",
    "inquirer": "^8.2.0",
    "ora": "^6.0.1",
    "rimraf": "^3.0.2",
    "slash": "^4.0.0",
    "tsup": "^5.11.11",
    "typescript": "^4.5.4",
    "vitest": "^0.0.139"
  }
}